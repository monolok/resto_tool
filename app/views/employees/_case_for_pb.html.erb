			<p class="click" data-toggle="modal" data-target=".28">Flexibility <strong style="color: red;">-<%=((1-(@uu.to_f/@ii.to_f))*100).round(0)%>%</strong><i class="fa fa-arrow-down red"></i></p>
		<%end%>
	<%when 29%>
		<% @ii = @list_pb[k].to_f%>
		<% @uu = @last_qu_hash[k].to_i%>
		<%if @uu > @ii %>
			<p class="click" data-toggle="modal" data-target=".29">Good to work with <strong style="color: green;">+<%=((1-(@uu.to_f/@ii.to_f))*100).round(0)*-1%>%</strong><i class="fa fa-arrow-up green"></i></p>
		<%elsif @uu == @ii%>
			<p class="click" data-toggle="modal" data-target=".29">Good to work with<strong style="color: brown;"> ━ </strong></p>			
		<%else%>
			<p class="click" data-toggle="modal" data-target=".29">Good to work with <strong style="color: red;">-<%=((1-(@uu.to_f/@ii.to_f))*100).round(0)%>%</strong><i class="fa fa-arrow-down red"></i></p>
		<%end%>
	<%else%>
		<p>Error</p>
	<%end%>
<%end%>
